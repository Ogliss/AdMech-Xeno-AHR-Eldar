<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<FactionDef Name="OG_Aeldari_CraftworldRefugee_Faction_Base" ParentName="FactionBase">
		<defName>OG_Aeldari_CraftworldRefugee_Faction</defName>
		<label>asuryani refugee group</label>
		<pawnSingular>refugee</pawnSingular>
		<pawnsPlural>refugees</pawnsPlural>
		<categoryTag>OG_OutlanderRefugee_Craftworld</categoryTag>
		<canMakeRandomly>false</canMakeRandomly>
		<canSiege>false</canSiege>
		<canStageAttacks>false</canStageAttacks>
		<hidden>true</hidden>
		<description>A group of eldar claiming to have lost their home.</description>
		<colorSpectrum>
			<li>(0.93, 0.39, 0.32)</li>
			<li>(0.35, 0.90, 0.57)</li>
			<li>(0.25, 0.66, 0.94)</li>
			<li>(0.98, 0.75, 0.37)</li>
			<li>(0.97, 0.62, 0.52)</li>
		</colorSpectrum>
		<generateNewLeaderFromMapMembersOnly>true</generateNewLeaderFromMapMembersOnly>
		<factionNameMaker>OG_Eldar_Craftworld_NamerFaction_Refugee</factionNameMaker>
		<leaderTitle>leader</leaderTitle>
		<requiredCountAtGameStart>0</requiredCountAtGameStart>
		<mustStartOneEnemy>false</mustStartOneEnemy>
		<maxCountAtGameStart>0</maxCountAtGameStart>
		<techLevel>Industrial</techLevel>
		<basicMemberKind>Refugee</basicMemberKind>
		<geneticVariance>0.8</geneticVariance>
		<backstoryFilters>
			<li>
				<categories>
					<li>Eldar_Standard</li>
				</categories>
				<commonality>0.95</commonality>
			</li>
			<li>
				<categories>
					<li>Eldar_Psyker</li>
				</categories>
				<commonality>0.05</commonality>
			</li>
		</backstoryFilters>
		<allowedCultures IgnoreIfNoMatchingField="True">
			<li>OG_Aeldari_Asuryani</li>
		</allowedCultures>
		<disallowedMemes IgnoreIfNoMatchingField="True">
			<li MayRequire="Ludeon.RimWorld.Ideology">Structure_Ideological</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Transhumanist</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
		</disallowedMemes>
		<disallowedPrecepts IgnoreIfNoMatchingField="True">
			<li MayRequire="Ludeon.RimWorld.Ideology">Nudity_Male_CoveringAnythingButGroinDisapproved</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Nudity_Female_CoveringAnythingButGroinDisapproved</li>
		</disallowedPrecepts>
		<structureMemeWeights IgnoreIfNoMatchingField="True" Inherit="False">
			<Structure_TheistAbstract MayRequire="Ludeon.RimWorld.Ideology">10</Structure_TheistAbstract>
		</structureMemeWeights>
		<hairTags IgnoreIfNoMatchingField="True">
			<li>Civil</li>
			<li>Raider</li>
			<li>Rural</li>
		</hairTags>
		<startingGoodwill IgnoreIfNoMatchingField="True">0</startingGoodwill>
		<naturalColonyGoodwill IgnoreIfNoMatchingField="True">-50~50</naturalColonyGoodwill>
		<settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
		<factionIconPath>Icons/Eldar/Icon_Eldar_Craftworld</factionIconPath>
	</FactionDef>

</Defs>