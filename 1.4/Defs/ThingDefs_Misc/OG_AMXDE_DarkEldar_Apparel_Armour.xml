	<!-- ====== Wych Armor ====== -->
	<!-- Add 0.55 inv save, increased in CQC to 0.92 -->
	<ThingDef ParentName="OG_DarkEldar_Apparel_Armor_Makeable">
		<defName>OGDE_Apparel_Armor_Wych</defName>
		<label>wych armor</label>
		<techLevel>Spacer</techLevel>
		<description>In the craft of murder, Wyches have no equal. In battle, Wyches feed upon screams of pain. With combat drugs coursing through their veins, Wyches roll, backflip and pirouette out of harm's way, stabbing through visors and neck-joints, slicing open a throat here and piercing a heart there.</description>
		<graphicData>
			<texPath>Things/Item/Apparel/DarkEldar/WychArmour/WychArmour__Icon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<statBases>
			<Mass>4</Mass>
			<WorkToMake>2800</WorkToMake>
			<MaxHitPoints>800</MaxHitPoints>
			<MarketValue>1000</MarketValue>
			<ArmorRating_Sharp>0.55</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.55</ArmorRating_Blunt>
			<ArmorRating_Heat>0.55</ArmorRating_Heat>
			<OG_ArmorRating_InvunerableSave>0.55</OG_ArmorRating_InvunerableSave>
			<Insulation_Cold>10</Insulation_Cold>
			<Insulation_Heat>5</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<equippedStatOffsets>
            <MoveSpeed>0.35</MoveSpeed>
            <CarryingCapacity>15</CarryingCapacity>
			<PainShockThreshold>0.25</PainShockThreshold>
			<MeleeDodgeChance>0.15</MeleeDodgeChance>
			<MeleeHitChance>0.15</MeleeHitChance>
			<PsychicSensitivity>-0.25</PsychicSensitivity>
			<ToxicResistance>-1</ToxicResistance>
		</equippedStatOffsets>
		<costList>
			<Steel>120</Steel>
			<Plasteel>40</Plasteel>
			<ComponentIndustrial>8</ComponentIndustrial>
			<OGDE_Wraithbone>30</OGDE_Wraithbone>
			<OGDE_SpiritStone>1</OGDE_SpiritStone>
			<OGDE_Component>10</OGDE_Component>
		</costList>
		<recipeMaker>
			<skillRequirements>
				<Crafting>15</Crafting>
			</skillRequirements>
			<researchPrerequisites>
				<li>OG_Aeldari_Tech_Apparel_Armour_T1</li>
			</researchPrerequisites>
		</recipeMaker>
		<apparel>
		<useWornGraphicMask>true</useWornGraphicMask>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li MayRequire="CETeam.CombatExtended">Hands</li>
				<li MayRequire="CETeam.CombatExtended">Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Item/Apparel/DarkEldar/WychArmour/WychArmour</wornGraphicPath>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>DarkEldarMilitary</li>
				<li>DEHaemonculusArmour</li>
				<li>EVA</li>
			</tags>
			<defaultOutfitTags>
				<li>DarkEldarSoldier</li>
			</defaultOutfitTags>
		</apparel>
		<thingCategories>
			<li>OGDEArmour</li>
		</thingCategories>
		<tradeTags>
			<li>OGDEArmour</li>
		</tradeTags>
		<comps>
		  <!--li Class="AdeptusMechanicus.CompProperties_PauldronDrawer">
			<compClass>AdeptusMechanicus.CompPauldronDrawer</compClass>
				<PauldronEntries>
