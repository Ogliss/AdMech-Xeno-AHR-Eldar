  <FactionDef Parent="PlayerFactionBase" Name="OG_Eldar_Player_Craftworld_Base"  Abstract="True">
    <label>Craftworld Eldar</label>
    <description>Your own community.</description>
    <isPlayer>true</isPlayer>
    <basicMemberKind>Colonist_OG_Eldar_Craftworld</basicMemberKind>
    <pawnsPlural>colonists</pawnsPlural>
		<playerInitialSettlementNameMaker>NamerInitialSettlementColony</playerInitialSettlementNameMaker>
		<factionNameMaker>NamerOGEldarFaction</factionNameMaker>
    <settlementNameMaker>NamerOGEldarBases</settlementNameMaker>
    <techLevel>Ultra</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>Eldar_Craftworld</li>
        </categories>
      </li>
    </backstoryFilters>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
	<factionIconPath>Icons/Icon_Eldar_Craftworld</factionIconPath>
    <startingResearchTags>
      <li>ClassicStart</li>
    </startingResearchTags>
    <hairTags>
			<li>EldarHair</li>
			<li>Rural</li>
    </hairTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Synthread</li>
      </thingDefs>
    </apparelStuffFilter>
    <colorSpectrum>
      <li>(1, 0.437, 0.847)</li>
    </colorSpectrum>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
  </FactionDef>
  
	
  <FactionDef ParentName="OG_Eldar_Player_Craftworld_Base">
    <defName>OG_Eldar_Player_Craftworld</defName>
  </FactionDef>

