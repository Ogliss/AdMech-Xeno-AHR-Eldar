  <AlienRace.BackstoryDef Name="Eldar_Base_Child" Abstract="True">
    <defName>Eldar_Base_Child</defName>
    <title>Eldar child</title>
    <titleShort>Eldar</titleShort>
    <baseDescription>[PAWN_nameDef] was born an Eldar. A race that sailed the stars long before huanites ancestors even looked up at the stars in wonder. now a shadow of their former glory and struggling for mere survival.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
    <slot>Childhood</slot>
    <workDisables>
    </workDisables>
    <disallowedTraits>
        <li>
            <defName>PsychicSensitivity</defName>
            <degree>-2</degree>
            <chance>100</chance>
        </li>
        <li>
            <defName>PsychicSensitivity</defName>
            <degree>-1</degree>
            <chance>100</chance>
        </li>
        <li>
            <defName>Beauty</defName>
            <degree>-2</degree>
            <chance>100</chance>
        </li>
        <li>
            <defName>Beauty</defName>
            <degree>-1</degree>
            <chance>100</chance>
        </li>
    </disallowedTraits>
    <skillGains>
	
    </skillGains>
    <spawnCategories>
      <li>Eldar</li>
      <li>Eldar_Child</li>
      <li>Eldar_Base_Child</li>
    </spawnCategories>
  </AlienRace.BackstoryDef>

