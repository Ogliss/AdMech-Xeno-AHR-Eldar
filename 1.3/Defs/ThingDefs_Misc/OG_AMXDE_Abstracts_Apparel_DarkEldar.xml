<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- ========================= Dark Eldar ========================== -->
    <!-- ========================= Dark Eldar - Clothes Base - Apparel Base ========================== -->
	<ThingDef Name="OG_DarkEldar_Apparel_Base" ParentName="OG_Apparel_Base" Abstract="True">
		<apparel>
			<ideoDesireAllowedFactionCategoryTags IgnoreIfNoMatchingField="True">
				<li>OG_Outlander_DarkEldar</li>
				<li>OG_OutlanderRefugee_DarkEldar</li>
			</ideoDesireAllowedFactionCategoryTags>
			<tags>
				<li>AeldariApparel</li>
				<li>DarkEldarApparel</li>
			</tags>
		</apparel>
		<thingCategories>
			<li>OGDESub</li>
		</thingCategories>
	</ThingDef>
	
    <!-- ========================= Dark Eldar - Clothes Base - Apparel Makable Base ========================== -->
	<ThingDef Name="OG_DarkEldar_Apparel_Makeable" ParentName="OG_DarkEldar_Apparel_Base" Abstract="True">
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
			<researchPrerequisites>
				<li>OG_DarkEldar_Tech_Base_T1</li>
			</researchPrerequisites>
		</recipeMaker>
	</ThingDef>
		
    <!-- ========================= Dark Eldar - Clothes - Apparel Makable ========================== -->
	<ThingDef Name="OG_DarkEldar_Apparel" ParentName="OG_DarkEldar_Apparel_Makeable" Abstract="True">
